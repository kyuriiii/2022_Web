<html>
    <head>
        <meta charset="UTF-8" />
        <title>EJS Example</title>
    </head>

    <body>
        <h1><%=title%> </h1>
        
        <% animals.forEach( (item) => { %> 
            <figure>
                <img src="<%=item.image%>" width="300px">
                <br />
                <figurecaption>
                    <%=item.imgtitle %>
                </figurecaption>
            </figure>
        <% }); %>
    </body>
</html>